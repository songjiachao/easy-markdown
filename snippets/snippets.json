{
  "# 标题": {
    "prefix": "/ 标题一   | H1",
    "body": [
      "# $1",
      "$0"
    ],
    "description": "一级标题"
  },
  "## 标题": {
    "prefix": "/ 标题二   | H2",
    "body": [
      "## $1",
      "$0"
    ],
    "description": "二级标题"
  },
  "### 标题": {
    "prefix": "/ 标题三   | H3",
    "body": [
      "### $1",
      "$0"
    ],
    "description": "三级标题"
  },
  "#### 标题": {
    "prefix": "/ 标题四   | H4",
    "body": [
      "#### $1",
      "$0"
    ],
    "description": "四级标题"
  },
  "*斜体*": {
    "prefix": "/ 斜体     | Italic",
    "body": [
      "*$1*$0"
    ],
    "description": "斜体"
  },
  "**加粗**": {
    "prefix": "/ 加粗     | Bold",
    "body": [
      "**${1:文本}**$0"
    ],
    "description": "加粗"
  },
  "***粗斜体***": {
    "prefix": "/ 粗斜体   | Italic & Bold",
    "body": [
      "***$1***$0"
    ],
    "description": "粗斜体"
  },
  "~~删除~~": {
    "prefix": "/ 删除     | Delete",
    "body": [
      "~~$1~~$0"
    ],
    "description": "删除"
  },
  "- 无序列表": {
    "prefix": "/ 无序列表 | UL",
    "body": [
      "* $1",
      "* $2",
      "* $3",
      "$0"
    ],
    "description": "无序列表"
  },
  "1. 有序列表": {
    "prefix": "/ 有序列表 | OL",
    "body": [
      "1. $1",
      "2. $2",
      "3. $3",
      "$0"
    ],
    "description": "有序列表"
  },
  "> 引用": {
    "prefix": "/ 引用     | Quote",
    "body": [
      "> $1",
      "$0"
    ],
    "description": "引用"
  },
  "[链接描述](url)": {
    "prefix": "/ 链接     | Link",
    "body": [
      "[${1:描述}](${2:url})$0"
    ],
    "description": "链接"
  },
  "![alt](url 图片描述)": {
    "prefix": "/ 图片     | Image",
    "body": [
      "![${2:描述}](${1:图片地址})",
      "$0"
    ],
    "description": "图片"
  },
  "`代码`": {
    "prefix": "/ 代码     | Code",
    "body": [
      "`$1`$0"
    ],
    "description": "代码"
  },
  "```编程语言↵": {
    "prefix": "/ 代码块   | Code Block",
    "body": [
      "```$1",
      "$2",
      "```",
      "$0"
    ],
    "description": "代码块"
  },
  "---分割线": {
    "prefix": "/ 分割线   | Split Line",
    "body": [
      "---",
      "$0"
    ],
    "description": "分割线"
  },
  "- [ ] 任务列表": {
    "prefix": "/ 任务列表 | Todo",
    "body": [
      "- [${1| ,x|}] ${2:text}",
      "- [${3| ,x|}] ${4:text}",
      "- [${5| ,x|}] ${6:text}",
      "$0"
    ],
    "description": "任务列表"
  },
  "| 表格 |  |": {
    "prefix": "/ 表格     | Table",
    "body": [
      "| 标题 |      |",
      "| ---- | ---- |",
      "|      |      |",
      "$0"
    ],
    "description": "表格"
  }
}